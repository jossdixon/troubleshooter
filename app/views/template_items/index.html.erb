<p id="notice"><%= notice %></p>

<h1>Template Items</h1>

<table>
  <thead>
    <tr>
      <th>Template</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @template_items.each do |template_item| %>
      <tr>
        <td><%= template_item.template_id %></td>
        <td><%= template_item.order %></td>
        <td><%= link_to 'Show', template_item %></td>
        <td><%= link_to 'Edit', edit_template_item_path(template_item) %></td>
        <td><%= link_to 'Destroy', template_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Template Item', new_template_item_path %>
